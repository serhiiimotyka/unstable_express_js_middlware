
const unstable_server = function (req, res, next) {
  function randomInteger(min, max) {
    return Math.floor(Math.random() * (max - min + 1)) + min;
  }
  if (Math.random() < 0.5) {
    setTimeout(() => {
      next()
    }, randomInteger(0, 2000))
  } else {
    res.status(403).send('ERROR')
  }
}
